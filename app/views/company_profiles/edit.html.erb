<h1>Edit Company Profile</h1>

<%= form_with model: @profile, url: company_profile_path do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  
  <%= f.label :address %>
  <%= f.text_field :address %>
  
  <%= f.label :website %>
  <%= f.text_field :website %>
  
  <%= f.label :description %>
  <%= f.text_area :description %>
  
  <%= f.submit "Update Profile" %>
<% end %>
